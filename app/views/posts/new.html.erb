<h1>New Post</h1>
<p></p>
<p>Author: <%= current_user.first_name %></p>


<%= form_for([current_user, @post]) do |f| %>
  <%= f.label "Blog Title" %>
  <%= f.text_field :title %>
  <p>
    <%= f.label "Content" %>
    <p>
    	<%= f.text_area :content %>
    	<%= f.submit "Create Post" %>
    </p>
  </p>
<% end %>