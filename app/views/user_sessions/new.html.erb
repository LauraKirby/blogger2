
<h1>Log In</h1>

<%= form_for @user_session do |f| %>
  <%= render 'shared/errors', object: @user_session %>


    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.label :remember_me %>
    <%= f.check_box :remember_me %>

  <%= f.submit "Log in!" %>
<% end %>